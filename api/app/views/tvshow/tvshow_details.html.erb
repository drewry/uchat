<div class="container">
  <h1>{{tvshow.title}}</h1>
  <p>{{tvshow.description}}</p>
  <p>{{tvshow.schedule}}</p>
  <div id="TvShow_leftContent" class="row col-md-3"><img alt="100%x200" src="data:image/svg+xml;base64," style="width: 100%; display: block;"/>
    <ul id="tvContentSwitch">
      <li id="showSchedule"><a href="#">Today's Showings</a></li>
      <li id="showOnDemand"><a href="#">On Demand</a></li>
      <li id="showTwitter"><a href="#">Twitter Community</a></li>
      <li id="showChat"><a href="#">Chat</a></li>
    </ul>
  </div>
  <div id="TvShow_ScheduledContent" class="col-md-9">
    <div class="pull-left">
      <h3 class="tvShowSchedule_Title">Scheduled air times</h3>
    </div>
    <div class="pull-right">
      <form class="form-inline">
        <div class="form-group">
          <input id="scheduleSearch" ng-model="searchText" placeholder="Is it showing today?" class="form-control"/>
        </div>
        <div class="form-group margin-left-med"></div>
      </form>
    </div>
    <table class="table table-hover table-striped table-condensed">
      <thead>
        <tr>
          <th>Channel</th>
          <th>Network</th>
          <th>Time</th>
          <th>Description</th>
        </tr>
        <tbody>
          <tr ng-repeat="tvshow in tvshows | filter:searchText | orderBy:sortOrder">
            <td>{{tvshow.channel}}</td>
            <td>{{tvshow.network}}</td>
            <td>{{tvshow.schedule | date}}</td>
            <td>{{tvshow.description}}</td>
          </tr>
        </tbody>
      </thead>
    </table>
  </div>
  <div id="TvShow_onDemandContent" class="col-md-9">
    <div class="pull-left">
      <h3 class="tvShowSchedule_Title">On Demand Content</h3>
    </div>
    <div class="pull-right">
      <form class="form-inline">
        <div class="form-group">
          <input id="onDemandSearch" ng-model="searchText" placeholder="Find a specific episode" class="form-control"/>
        </div>
        <div class="form-group margin-left-med"></div>
      </form>
    </div>
    <table class="table table-hover table-striped table-condensed">
      <thead>
        <tr>
          <th>Episode</th>
          <th>Description</th>
        </tr>
        <tbody>
          <tr ng-repeat="tvshow in tvshows | filter:searchText | orderBy:sortOrder">
            <td>{{tvshow.channel}}</td>
            <td>{{tvshow.description}}</td>
          </tr>
        </tbody>
      </thead>
    </table>
  </div>
