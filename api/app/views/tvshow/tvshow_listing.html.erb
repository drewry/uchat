<panel class="panel-primary">
  <div ng-repeat="tvshow in tvshows" class="showListingsWrapper"><a href="/tvshows/{{tvshow._id}}">
      <div class="thumbnail"><img alt="100%x200" src="data:image/svg+xml;base64," style="width: 100%; display: block;"/>
        <h4 class="tvShowBlock">{{tvshow.title}}</h4>
        <div class="caption">
          <p class="showDesc">{{tvshow.description}}</p>
          <hr/><a href="/tvshows/{{tvshow._id}}" role="button" class="btn btn-sm btn-link">showtimes </a><a href="#" role="button" class="btn btn-sm btn-primary">community </a>
        </div>
      </div></a></div>
</panel>